# Rebids after a jacoby 2n

# C = ♣
# D = ♦
# H = ♥
# S = ♠

common 1M 2N:"xfer" {
	4x => 5+ x, 2 of top 3 in x;
	    test AQxxx x KQxxx Ax:
	        1♠ 2N
	        4♦;
	
	3x => 1- x;
	    test AQxxx x KQxx Axx:
	        1♠ 2N
	        3♥;
	
	3N => 15-17 hcp;
	    test AQxxx xx KQxx Ax:
	        1♠ 2N
	        3N;
	
	3M => 18+ hcp;
	    test AQxxx Ax KQxx Ax:
	        1♠ 2N
	        3♠;
	
	4M => always;
	    test AQxxx xx Kxxx Ax:
	        1♠ 2N
	        4♠;
}