# 1 over 1 rebids

# C = ♣
# D = ♦
# H = ♥
# S = ♠

common 1x:seats12 NJy:>1N {
	2z:<2x => 4+z;
	    test AQxxx Axxx xx Kx:
	        1S 2D
	        2H;
	
	NJz:>2x => 16+ tpts, 4+z;
	    test Axxx AQxxx Ax Kx:
	        1H 2D
	        2S;
	    test Ax AQxxx Ax Kxxx:
	        1H 2D
	        3C;
	
	NJy => 4+y;
	    test Ax AQxxx Axxx Kx:
	        1H 2D
	        3D;
	
	2x => always;
	    test Axx AQxxx xxx Kx:
	        1H 2D
	        2H;
}
