common (1w) 1x (2w) {
    3w:"cuebid" => 11+ tpts, 3+x, 10+ hcp;
	    test xx AQxx AJxx xxx:
	        (1C) 1H (2C) 3C;
    3x => 0-9 tpts, 4+x;
	    test xx Axxx xxxx xxx:
	        (1C) 1H (2C) 3H;
    2x => 6-10 tpts, 3+x;
	    test xx AQx xxxxx xxx:
	        (1C) 1H (2C) 2H;
	NJy => 10+ tpts, 8+ hcp, 6+y;
	    test xxx AQJxxx Qx xx:
	        (1C) 1D (2C) 2H;
    x => 4+ y, 4+ z, 6+ tpts;
	    test Axxx Axxx xx xxx:
	        (1C) 1D (2C) X;
}
